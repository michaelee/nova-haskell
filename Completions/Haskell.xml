<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <provider>
        <syntax>haskell</syntax>
        <selector>haskell</selector>
        <expression>\b[a-zA-Z0-9-_]*</expression>
        <set>haskell.keywords</set>
        <set>haskell.prelude.functions</set>
